#include<stdio.h>
int main()
{
	int i ,n;
	printf("please input a number:");
	scanf("%d",&n);
	for(i = 2;i <= n-1;i++)
	{
		if(n % i == 0)
		break;
		//else
		//i = i + 1;     //如果这句没有，则有i++（for循环里）
	}
	if(i < n)
	printf("%d is not prime number\n",n);
	else
	printf("%d is prime number\n",n);
	return 0;
}
