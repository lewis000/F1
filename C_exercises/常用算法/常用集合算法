1、set_intersection--->集合求交集

#include<iostream>
#include<vector>
#include<algorithm>
using namespace std;

void print(int val)
{
	cout << val << " ";
}

void test01()
{
	vector<int>v1;
	vector<int>v2;
	vector<int>vt;
	v1.push_back(3);
	v1.push_back(11);
	v1.push_back(13);
	v1.push_back(33);
	v1.push_back(44);
	for (int i = 3; i < 14; i++)
	{
		v2.push_back(i);
	}
	//目标容器需要提前开辟空间
	//最特殊的情况，大容器包含小容器，开辟空间取小容器的size即可
	vt.resize(min(v1.size(), v2.size()));    //min取小函数，需要包含algorithm头文件

	//获取交集
	//set_intersection是针对有序序列的算法，两个容器数据顺序必须一致
	//set_intersection返回的是结束迭代器
	vector<int>::iterator itend = set_intersection(v1.begin(), v1.end(), v2.begin(), v2.end(), vt.begin());
	for_each(vt.begin(), itend, print);
	cout << endl;

	for_each(v1.begin(), v1.end(), print);
	cout << endl;
	for_each(v2.begin(), v2.end(), print);
	

}

int main()
{
	test01();
	return 0;
}

