#include<stdio.h>
int main()
{
	int i,j;
	int n;
	for(i = 1; i <= 4;i++)
	{
		for(j = 1; j <= 5;j++,n++)   //n++有与没有输出格式不一样？ 
		{
			n = n + 1;        //这句有与没有输出格式不一样？    10和14，同一句话，位置不同，输出格式不同
			if(n % 5 == 0) 
			printf("\n");    //该行为什么会对结果产生影响？ 
			printf("%d\t",i * j);
			n = n + 1;       //与n++输出格式一致 
		}
	}
	//printf("\n");   //此行有与没有无影响 
	return 0;
}
